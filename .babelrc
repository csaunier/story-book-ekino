{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "ie >= 11"],
          "node": "current"
        }
      }
    ],
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["src"]
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": ["dynamic-import-node"]
    }
  }
}
