{
  "private": true,
  "name": "react-redux-starter-kit",
  "description": "React redux starter kit",
  "version": "0.0.0",
  "author": "Ekino",
  "license": "ISC",
  "scripts": {
    "build": "rimraf public && NODE_ENV=production webpack --progress --hide-modules --config webpack/build.babel.js",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "dev": "NODE_ENV=development webpack-dev-server --progress --config webpack/dev.babel.js",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "lint:js": "eslint \"{src,webpack,.storybook}/**/*.{js,jsx}\"",
    "lint": "yarn lint:js && yarn lint:css",
    "precommit": "lint-staged",
    "prepush": "yarn test",
    "prettier": "prettier --write \"{src,webpack,.storybook}/**/*.{js,jsx,css,scss,json}\"",
    "start": "http-server public -o",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest src test"
  },
  "lint-staged": {
    "*.{css,scss}": [
      "yarn lint:css"
    ],
    "*.{js,jsx}": [
      "yarn lint:js --fix",
      "git add"
    ],
    "*.{js,jsx,css,scss,json}": [
      "yarn prettier",
      "git add"
    ]
  },
  "dependencies": {
    "@storybook/react": "^3.2.17",
    "classnames": "^2.2.5",
    "history": "^4.6.3",
    "i18next": "^10.2.1",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^1.5.0",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-i18next": "^7.1.1",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^5.2.3",
    "autoprefixer": "^7.2.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bundle-loader": "^0.5.5",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "happypack": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "js-yaml-loader": "^0.1.0",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.9.1",
    "prettier-stylelint": "^0.4.2",
    "react-hot-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^8.3.1",
    "stylelint-config-css-modules": "^1.1.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^2.2.0",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1",
    "yaml-jest": "^1.0.5",
    "yaml-loader": "^0.5.0"
  }
}
